{
  "wfh": {
    "date": [
      {
        "search": "^.*WFH.*[-:].*[0-9]{1,2}( )?.{1}( )?([0-9]{1,2}|[a-zA-Z]+)( )?.{1}( )?[0-9]{2,4}.*$",
        "extract": "[0-9]{1,2}( )?.{1}( )?([0-9]{1,2}|[a-zA-Z]+)( )?.{1}( )?[0-9]{2,4}"
      }
    ],
    "empcode": [
      {

        "search": "(employee )?code.*[0-9]{5}",
        "extract": "[0-9]{5}"
      }
    ],
    "timein": [
      {

        "search": "time( )?in.*[0-9]{1,2}.{1}[0-9]{1,2}.{1}([aApP][mM])?",
        "extract": "[0-9]{1,2}.{1}[0-9]{1,2}.{1}([aApP][mM])?"
      }
    ],
    "timeout": [
      {

        "search": "time( )?out.*[0-9]{1,2}.{1}[0-9]{1,2}.{1}([aApP][mM])?",
        "extract": "[0-9]{1,2}.{1}[0-9]{1,2}.{1}([aApP][mM])?"
      }
    ],
    "hours": [
      {

        "search": "hours.*[0-9]{1,2}([.,][0-9]{1,2})?",
        "extract": "[0-9]{1,2}([.,][0-9]{1,2})?"
      }
    ]
  }

}
